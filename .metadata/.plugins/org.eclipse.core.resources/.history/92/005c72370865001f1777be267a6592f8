package com.kosta.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
@RequestMapping("/yyj/*")
public class YackControlloer {
	
	@GetMapping("/btm")
	public String btm() {
		return "yyj/btm";
	}
	@GetMapping("/cez")
	public String cez() {
		return "yyj/cez";
	}
	@GetMapping("/mg")
	public String mg() {
		return "yyj/mg";
	}
	@GetMapping("/hs")
	public String hs() {
		return "yyj/hs";
	}
	@GetMapping("/cb")
	public String cb() {
		return "yyj/cb";
	}
}
